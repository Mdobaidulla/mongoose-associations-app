<h1>All Albums</h1>
<ul>
  <% albums.forEach(album => { %>
  <li><a href="/albums/<%= album.id %>"><%= album.name %></a></li>
  <!-- <a href="/users/<%= album.id %>/edit">Edit</a> -->
  <form action="/albums/<%= album.id %>?_method=DELETE" method="POST">
    <button>Delete</button>
  </form>

  <% }) %>
  
</ul>